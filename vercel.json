{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        }
    ],
    "routes": [
        {
      "src": "/(.*)",
      "dest": "/dist"
    },
        { "src": "/api/v1/", "dest": "/dist/index.js"},
    ],
    "rewrites": [{ "source": "/api/v1/(.*)", "destination": "/dist" }]
}
